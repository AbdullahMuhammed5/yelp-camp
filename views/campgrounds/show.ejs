<% include ../partials/header.ejs%>

<h1><%= campground.name%></h1>
<img src="<%= campground.image %>" class="img-fluid"> 
<p><%= campground.description%></p>

<% campground.comments.forEach(comment => { %>
    <p>
        <strong><%= comment.author %></strong>
        <%= comment.text %>
    </p>
<% }); %>

